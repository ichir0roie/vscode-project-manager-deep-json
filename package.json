{
  "name": "project-manager-deep-json",
  "displayName": "Project Manager Deep Json",
  "description": "",
  "publisher": "ichir0roie",
  "repository": "https://github.com/ichir0roie/vscode-project-manager-deep-json",
  "version": "3.0.0",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:projectManagerDeepJson.helloWorld",
    "onView:projectsExplorer",
    "onView:projectManagerDeepJson"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "projectManagerDeepJson.refreshJson",
        "title": "Refresh Json Project Setting",
        "icon": {
          "light": "resources/vscode-icons/icons/light/refresh.svg",
          "dark": "resources/vscode-icons/icons/dark/refresh.svg"
        }
      },
      {
        "command": "projectManagerDeepJson.openJson",
        "title": "Open Json Project Setting",
        "icon": {
          "light": "resources/vscode-icons/icons/light/settings.svg",
          "dark": "resources/vscode-icons/icons/dark/settings.svg"
        }
      },
      {
        "command": "projectManagerDeepJson.openWindowThis",
        "title": "Open Project With This Window",
        "icon": {
          "light": "resources/vscode-icons/icons/light/folder-opened.svg",
          "dark": "resources/vscode-icons/icons/dark/folder-opened.svg"
        }
      },
      {
        "command": "projectManagerDeepJson.openWindowNew",
        "title": "Open Project With New Window",
        "icon": {
          "light": "resources/vscode-icons/icons/light/folder-opened.svg",
          "dark": "resources/vscode-icons/icons/dark/folder-opened.svg"
        }
      },
      {
        "command": "projectManagerDeepJson.addProject",
        "title": "This Window Add Project",
        "icon": {
          "light": "resources/vscode-icons/icons/light/bookmark.svg",
          "dark": "resources/vscode-icons/icons/dark/bookmark.svg"
        }
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "projectManagerDeepJson",
          "title": "ProjectManager DeepJson",
          "icon": "resources/vscode-icons/icons/dark/project.svg"
        }
      ]
    },
    "views": {
      "projectManagerDeepJson": [
        {
          "id": "projectManagerDeepJson",
          "name": "Project-Manager Deep-Json"
        }
      ]
    },
    "configuration": [
      {
        "title": "Project Manager Deep Json",
        "properties": {
          "projectManagerDeepJson.projects.backup": {
            "type": "object",
            "description": "project tree backup"
          }
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "projectManagerDeepJson.refreshJson",
          "when": "view == projectManagerDeepJson",
          "group": "navigation"
        },
        {
          "command": "projectManagerDeepJson.openJson",
          "when": "view == projectManagerDeepJson",
          "group": "navigation"
        },
        {
          "command": "projectManagerDeepJson.addProject",
          "when": "view == projectManagerDeepJson",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "projectManagerDeepJson.openWindowThis",
          "when": "view == projectManagerDeepJson",
          "group": "inline"
        },
        {
          "command": "projectManagerDeepJson.openWindowNew",
          "when": "view == projectManagerDeepJson"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "jsonc": "^2.0.0",
    "strip-json-trailing-commas": "^1.1.0"
  }
}
