{
  "name": "project-manager-deep-json",
  "displayName": "Project Manager Deep Json",
  "description": "",
  "author": {
    "name": "ichir0roie"
  },
  "repository": {
    "url": "https://github.com/ichir0roie/vscode-project-manager-deep-json"
  },
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:projectManagerDeepJson.helloWorld",
    "onView:projectsExplorer",
    "onView:projectManagerDeepJson"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "projectManagerDeepJson.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "projectManagerDeepJson.refreshEntry",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "projectManagerDeepJson.addEntry",
        "title": "Add"
      },
      {
        "command": "projectManagerDeepJson.openEntry",
        "title": "Edit",
        "icon": {
          "light": "resources/light/edit.svg",
          "dark": "resources/dark/edit.svg"
        }
      },
      {
        "command": "projectManagerDeepJson.deleteEntry",
        "title": "Delete"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "projectManagerDeepJson",
          "title": "ProjectManager DeepJson",
          "icon": "images/project-manager-side-bar.svg"
        }
      ]
    },
    "views": {
      "projectManagerDeepJson": [
        {
          "id": "projectManagerDeepJson",
          "name": "ProjectManager DeepJson"
        }
      ]
    },
    "configuration": [
      {
        "title": "Project Manager Deep Json",
        "properties": {
          "projectManagerDeepJson.projects": {
            "type": "object",
            "description": "project tree"
          }
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "projectManagerDeepJson.refreshEntry",
          "when": "view == projectManagerDeepJson",
          "group": "navigation"
        },
        {
          "command": "projectManagerDeepJson.addEntry",
          "when": "view == projectManagerDeepJson"
        }
      ],
      "view/item/context": [
        {
          "command": "projectManagerDeepJson.openEntry",
          "when": "view == projectManagerDeepJson ",
          "group": "inline"
        },
        {
          "command": "projectManagerDeepJson.deleteEntry",
          "when": "view == projectManagerDeepJson "
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  }
}
